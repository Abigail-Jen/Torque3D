function TAIKGui::onWake(%this)
{
	if ($spawnTeam != 1)
	  Team0Radio.performClick();
	if ($spawnTeam == 1)
	  Team1Radio.performClick();
	
	PathList.clear();
	PathList.add("None",0);
	for(%i=0;%i<Paths.getCount();%i++)
	{
		PathList.add(Paths.getObject(%i).getName(),Paths.getObject(%i).getId());
	}
	
	EditPathList.clear();
	EditPathList.add("None",0);
	for(%i=0;%i<Paths.getCount();%i++)
	{
		EditPathList.add(Paths.getObject(%i).getName(),Paths.getObject(%i).getId());
	}
	
	ZoneList.clear();
	for(%i=0;%i<AIZones.getCount();%i++)
	{
		%zone = AIZones.getObject(%i);
		if (%zone.getDatablock().getName() $= "AIZone")
		{
			ZoneList.add(%zone.getName(),%zone);
		}
	}
	ZoneList.setSelected($curSetupZone);
	
	//if ($pathToAddTo)
		EditPathList.setSelected($pathToAddTo);
	//if ($markerAddPath)
          $markerAddPath = 0;
	  PathList.setSelected($markerAddPath);

	getLastNavNodeDist();
	refreshAdjList();
}

