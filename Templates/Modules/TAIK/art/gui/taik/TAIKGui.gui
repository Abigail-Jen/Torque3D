function getLastNavNodeDist()
{
	%node = $AIPaths::LastDroppedNode;
	if (!isObject(%node))
	{
		LastNodeDist.setText("");
	  return;
	}
	  
	%dist = VectorDist(LocalClientConnection.player.getPosition(),%node.getPosition());
	LastNodeDist.setText("Distance to most recently created nav node (" @ %node @ ") : " @ %dist);
}

function removeNavNode(%node)
{
	if (!isObject(%node))
	  return;
	  
	AIPaths.removeObjectSafe(%node);
  %node.schedule(200,delete);  
}

function removeEnteredNode()
{
	%node = RemoveNodeEntry.getText();
	removeNavNode(%node);
	if (isObject(%node))
	  RemoveNodeEntry.setText("");
}

function removeRecentNode()
{
	%count = AIPaths.getCount();
	%idx = %count - 1;
	%node = AIPaths.getObject(%idx);
	removeNavNode(%node);
}

function createAdjacency()
{
	if (!isObject(AdjAdd1.getText()) || !isObject(AdjAdd2.getText()))
	{
		return;
	}
	AIPaths.createAdjPair(AdjAdd1.getText(),AdjAdd2.getText());
	AdjAdd1.setText("");
	AdjAdd2.setText("");
}

function removeAdjacency()
{
	if (!isObject(AdjRem1.getText()) || !isObject(AdjRem2.getText()))
	{
	  return;
	}
	  
	AIPaths.removeAdjPair(AdjRem1.getText(),AdjRem2.getText());
	AdjRem1.setText("");
	AdjRem2.setText("");
}

function EditorNewPatrolPath()
{
	NewAIPath();
	EditPathList.add($pathToAddTo.getName(),$pathToAddTo);
	EditPathList.setSelected($pathToAddTo);
}

function refreshAdjList()
{
	if (!$curSetupZone)
	{
		AdjListText.clear();
	  return;
	}

	 AdjListText.clear();
	for (%i = 0;%i < getWordCount($curSetupZone.adjacentZones);%i++)
	{
		%adj = getWord($curSetupZone.adjacentZones,%i);
		%adj = %adj.getId();
		error("a is " @ %adj);
		if (isObject(%adj))
		{
			AdjListText.addRow(%adj.getId(),%adj.getName());
		}
	}
}

function setCurZone()
{
	%zone = ZoneList.getSelected();
	$curSetupZone = %zone;
	refreshAdjList();
}

function createAIZonesGroup()
{
  if (isObject(AIZones))
    return;
  new SimGroup(AIZones) {};
  MissionGroup.add(AIZones);
}